<%- include("header") -%>

<div class="box heading" id="main-heading">
  <h1>Welcome</h1>
</div>

<div class="container">
  <div class="row align-items-start">

    <% console.log(lists) %> 

    <% lists.forEach(list => { %>
        
        <div class="col-lg-4 col-md-6 col-sm-12 px-4">
          <div class="box heading">
            <h2> <%= list.name %> </h2>
          </div>
    
          <div class="box">

            <% list.items.forEach(item => { %>
                <div class="item">
                  <p> <%= item %> </p>
                </div>
             
            <% }) %>

          </div>
        </div>

    <% }) %>

  </div>

<%- include("footer") -%>